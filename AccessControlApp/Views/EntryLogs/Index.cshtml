@model IEnumerable<AccessControlApp.Models.Entities.EntryLog>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Device.Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PointOfAccess.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Success)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Device.Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PointOfAccess.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Success)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
